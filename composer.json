{
    "name": "alibori/laravel-api-resource-generator",
    "description": "Package to generate API resources from models.",
    "authors": [
        {
            "name": "Axel Libori Roch",
            "email": "a.libori@gmail.com"
        }
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Alibori\\LaravelApiResourceGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alibori\\LaravelApiResourceGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Alibori\\LaravelApiResourceGenerator\\LaravelApiResourceGeneratorServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "resources",
        "generator"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "doctrine/dbal": "^2.9|^2.10|^3.0",
        "illuminate/database": "^9.1",
        "barryvdh/reflection-docblock": "^2.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^7.22",
        "laravel/pint": "^1.6",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.4"
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
